<view><swiper style="height:300rpx;width:100%;" indicator-dots="{{true}}" autoplay="{{true}}" circular="{{true}}"><block wx:for="{{swiperList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><swiper-item><view class="swiper-item"><image src="{{item.image}}"></image></view></swiper-item></block></swiper><view class="sort-postion">-导航栏-</view><view class="nav-list"><block wx:for="{{sortList}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['navClickHandler',['$0'],[[['sortList','',i]]]]]]]}}" class="nav-item" bindtap="__e"><image class="nav-img" src="{{item.image}}"></image><text class="nav-text">{{item.name}}</text></view></block></view><view class="total-postion">-全部职业-</view><block wx:for="{{totallist}}" wx:for-item="item" wx:for-index="i" wx:key="_id"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['totallist','_id',item._id,'_id']]]]]]]}}" class="potion-item" bindtap="__e"><view class="piece-text-sort"><text>{{item.sort}}</text></view><view class="piece-text-address"><text>{{item.address}}</text></view><view class="piece-text-salary"><text>{{item.salary}}</text></view></view></block><block wx:if="{{$root.g0===34}}"><view class="warn">到底了哦~</view></block></view>